# ---- Common Ignore ---- #
# Ignore all system-specific files
.DS_Store
Thumbs.db

# Logs
logs/
*.log

# Ignore environment variable files
*.env

# Ignore IDE config files (optional)
.vscode/
.idea/
*.iml

# ---- Frontend (React.js) Ignore ---- #
rss-feed-frontend/node_modules/
rss-feed-frontend/build/
rss-feed-frontend/.next/
rss-feed-frontend/.parcel-cache/

# Ignore npm-debug files
rss-feed-frontend/npm-debug.log*
rss-feed-frontend/yarn-debug.log*
rss-feed-frontend/yarn-error.log*

# Ignore local environment files for React
rss-feed-frontend/.env.local
rss-feed-frontend/.env.development.local
rss-feed-frontend/.env.test.local
rss-feed-frontend/.env.production.local

# Ignore coverage files generated by tests
rss-feed-frontend/coverage/

# ---- Backend (Java Spring Boot) Ignore ---- #
rss-feed-backend/target/
rss-feed-backend/out/
rss-feed-backend/build/

# Ignore class files and other build artifacts
*.class
*.jar
*.war
*.ear

# Ignore Maven-specific files
rss-feed-backend/.mvn/
rss-feed-backend/target/
rss-feed-backend/pom.xml.tag
rss-feed-backend/pom.xml.releaseBackup
rss-feed-backend/pom.xml.versionsBackup
rss-feed-backend/pom.xml.next
rss-feed-backend/release.properties

# Ignore Gradle-specific files (if you use Gradle)
rss-feed-backend/.gradle/
rss-feed-backend/build/

# Ignore IDE or Eclipse-specific files (if using an IDE like IntelliJ or Eclipse)
*.classpath
*.project
*.settings/
*.factorypath

# Ignore generated files like Swagger API docs
rss-feed-backend/generated/

# Ignore local application.properties for Spring Boot (if you have environment-specific configurations)
rss-feed-backend/src/main/resources/application-*.properties
rss-feed-backend/src/main/resources/application-*.yml

# Ignore test coverage output
rss-feed-backend/coverage/
rss-feed-backend/jacoco/

# Ignore any temporary files during packaging
rss-feed-backend/*.log